 "css": ".body_one{\n    height:100%;\n    width:100%;\n}\n.footer{\n    position: absolute;\n    bottom: 0;\n    margin-left:10%;\n}\n#div_tab{\n    width:100%;\n    color:black;\n    margin-top:10px;\n    font-size:12px;\n}\n.body_two{\n    font-size:12px; \n    color:black; \n    margin-top:20px;\n    text-align:left;\n    line-height: 1.0;\n}\n.table1{\n\ttext-align:justify;\n\tfont-size:small;\n\twidth:100%;\n}\nth{\n    background:#E6E6E6;\n    color:black;\n    font-size:12px;\n}\n#col{\n    border:1px solid #CBCBCB;\n\tborder-collapse: collapse;\n}\n#less{\n    width:20%;\n}\n#more{\n    width:40%;\n}\n#mo{\n    width:20%;\n}\n#in_word{\n    height:100%;\n    width:100%;\n    color:black;\n    padding-top:20px;\n}",
