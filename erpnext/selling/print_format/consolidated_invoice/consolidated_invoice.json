 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">CONSOLIDATED INVOICE</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{ doc.cost_center }}</p>\r\n    </div>\r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Customer Name:</strong></td>\r\n                <td>{{doc.customer}}</td>\r\n                <td><strong>Consolidated Invoice Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Consolidated Invoice No:</strong></td>\r\n                <td id =\"more\">{{doc.name}}</td>\r\n                <td id =\"less\"><strong>From Date:</strong></td>\r\n                <td id =\"mo\">{{doc.from_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong></strong></td>\r\n                <td id =\"more\"></td>\r\n                <td id =\"less\"><strong>To Date.:</strong></td>\r\n                <td id =\"mo\">{{ doc.to_date }}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Item Code</strong></th>\r\n                <th id=\"col\"><strong>Item Name</strong></th>\r\n                <th id=\"col\"><strong>Accepted Qty.</strong></th>\r\n                <th id=\"col\"><strong>Rate.</strong></th>\r\n                <th id=\"col\"><strong>UoM</strong></th>\r\n                <th id=\"col\"><strong>Amount Billed (Nu.)</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                <td id=\"col\">1</td>\r\n                <td id=\"col\">{{doc.item_code}}</td>\r\n                <td id=\"col\">{{doc.item_name}}</td>\r\n                <td id=\"col\">{{doc.quantity}}</td>\r\n                <td id=\"col\">{{doc.rate}}</td>\r\n                <td id=\"col\">{{doc.uom}}</td>\r\n                <td id=\"col\">{{doc.total_amount}} </td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"col\" colspan=\"6\"><strong>Other Charges</strong></td>\r\n                <td id =\"col\"><strong>{{doc.loading_amount}}</strong></td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"col\" colspan=\"6\"><strong>Grand Total</strong></td>\r\n                <td id =\"col\"><strong>{{doc.grand_total}}</strong></td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    \r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words:</strong>Ngultrum {{frappe.utils.money_in_words(doc.grand_total)}}\r\n    </div>\r\n    \r\n    <div style=\"padding-top: 100px; clear: both;\">\r\n        <div>Authorized Signatory</div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Invoice Raised By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Verified By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Acknowledged By) </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
