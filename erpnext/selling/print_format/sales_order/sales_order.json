 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">SALES ORDER</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{ doc.branch }}</p>\r\n    </div>\r\n    <div style=\"float:left; width: 100%;\">\r\n        <p style=\"margin-left:80%;font-size:14px\"><b>Date:{{ doc.transaction_date }}</b></p>\r\n    </div>\r\n    <div class=\"body_div\" style=\"padding-top:20px\">\r\n        <div class=\"body_head_outer\">\r\n            <div class=\"head_in1\" style=\"font-size:14px; color:black; text-align:left;height:80%;line-height: 1.5;\">\r\n                <p>{{doc.customer}}</p>\r\n                <p>Mobile Number/Fax: {{ frappe.db.get_value(\"Customer\", doc.customer, \"mobile_no\") }}</p>\r\n                <p>{{frappe.db.get_value(\"Customer\", doc.customer, \"primary_address\") }}</p><br>\r\n                <p><b>Subject</b>:{{doc.title}}</p>\r\n            </div>\r\n        </div>\r\n        <div style=\"clear: both; margin-top: 1px;\"><br/><p>Dear Sir/Madam,</p>\r\n            {%- if doc.sales_order_header_text -%}\r\n                {{ doc.sales_order_header_text }}\r\n            {%- endif -%}\r\n        </div>\r\n        <div id=\"div_table\">\r\n            <table class=\"table_items\">\r\n                <tbody>\r\n                    <tr>\r\n                        <th style=\"text-align: center;\">Sl No</th>\r\n                        <th style=\"text-align: center;\">Item Code</th>\r\n                        <th style=\"text-align: center;\">Item Name</th>\r\n                        <th style=\"text-align: center;\">Unit</th>\r\n                        <th style=\"text-align: center;\">Delivery Date</th>\r\n                        <th style=\"text-align: center;\">Quantity</th>\r\n                        <th style=\"text-align: center;\">Rate</th>\r\n                        <th style=\"text-align: center;\">Amount</th>\r\n                    </tr>\r\n            \t    {%- for row in doc.items -%}\r\n                    <tr>\r\n                        <td>{{row.idx}}</td>\r\n                        <td>{{row.item_code}}</td>\r\n                        <td>{{row.item_name}}</td>\r\n                        <td>{{row.uom}}</td>\r\n                        <td>{{row.delivery_date}}</td>\r\n                        <td>{{row.qty}}</td>\r\n                        <td>{{row.rate}}</td>\r\n                        <td>{{row.amount}}</td>\r\n                    </tr>\r\n                    {%- endfor -%}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n        <div style=\"margin-top:20px\">\r\n            {%- if doc.sales_order_footer_text -%}\r\n                {{ doc.sales_order_footer_text }}\r\n            {%- endif -%}\r\n        </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
