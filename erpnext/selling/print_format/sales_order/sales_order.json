 "css": ".body_one{\n    height:100%;\n    width:100%;\n}\n.head_outer{\n    /*height:110px;*/\n    width:90%;\n    margin-left:40px;\n}\n.head_in1{\n    height:100%;\n    width:100%;\n    margin: auto;\n    float: left;\n}\n.body_div{\n    height:100%;\n    width:90%;\n    margin-left:40px;\n}\n.footer{\n    position: absolute;\n    bottom: 0;\n    margin-left:10%;\n}\ntr,td,th{\n    border:1px solid black;\n    border-collapse: collapse;\n    font-size:14px;\n    color:black;\n}\n.table_items{\n    width:100%;\n}\n#div_table{\n    height:100%;\n    width:100%;\n    color:black;\n    padding-top:20px;\n}",
