 "css": "table {\n    margin: 0 auto; /* or margin: 0 auto 0 auto */\n    width: 100%;\n  }\n\n#table2 th, #table2 td {\n    border: 1px solid black;\n}\n\n#table2 th {\n    font-weight: bold;\n    text-transform: capitalize;\n}\n\n#main {\n   margin-left: 20px;\n   margin-right: 20px;\n}\n@media all {\n   div .printedby {\n       position: fixed;\n       bottom: 0;\n       text-align: center;\n       font-size: xx-small;\n       width: 100% !important;\n   }\n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Purchase Receipt",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<div class=\"page-break\">\r\n    <div id=\"header-html\" class=\"hidden-pdf\">\r\n        {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\">\r\n            {{ letter_head }}\r\n        </div>\r\n        {%- endif -%}\r\n    </div>\r\n    <div id=\"main\">\r\n        <div style=\"text-align: center;\">\r\n            <h3>PURCHASE RECEIPT</h3>\r\n            <h5><u>\r\n                    {%- if doc.items[0].cost_center -%}\r\n                    {{ doc.items[0].cost_center }} {%- endif -%}\r\n                </u></h5>\r\n        </div>\r\n        <div style=\"margin-top: 10px;\">\r\n            <table style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n                <tr>\r\n                    <td><strong>Vendor Name:</strong></td>\r\n                    <td>{{ doc.supplier }}</td>\r\n                    <td><strong>PR No.:</strong></td>\r\n                    <td>{{ doc.name }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>PO No:</strong></td>\r\n                    <td>{{ doc.items[0].purchase_order }}</td>\r\n                    <td><strong>PR Date:</strong></td>\r\n                    <td>{{ frappe.utils.formatdate(doc.posting_date) }}</td>\r\n                </tr>\r\n                <tr>\r\n                    <td><strong>PO Date:</strong></td>\r\n                    <td>{{ frappe.utils.formatdate(frappe.db.get_value(\"Purchase Order\",doc.items[0].purchase_order,\r\n                        \"transaction_date\")) }}</td>\r\n                    <td><strong>Vendor Invoice No.:</strong></td>\r\n                    <td>{%- if doc.bill_no -%} {{ doc.bill_no }} {%- endif -%}</td>\r\n                </tr>\r\n                <tr>\r\n\r\n                    <td><strong>Vendor Invoice Date:</strong></td>\r\n                    <td>{%- if doc.bill_date -%} {{ frappe.utils.formatdate(doc.bill_date) }} {%- endif -%}</td>\r\n                    <td><strong> Actual Receipt Date:</strong></td>\r\n                    <td>{{ frappe.utils.formatdate(doc.actual_receipt_date) }}</td>\r\n                </tr>\r\n                <tr>\r\n\r\n                    <td><strong>GRN Text:</strong></td>\r\n                    <td>{{ doc.title }}</td>\r\n                <tr>\r\n                    <td><strong> Currency: </strong> </td>\r\n                    <td>{{doc.currency}}</td>\r\n                    {%- if doc.lr_no -%}\r\n                    <td><strong>Vehicle No.:</strong></td>\r\n                    <td>{{doc.lr_no}}</td>\r\n                    {%- endif-%}\r\n                </tr>\r\n                <tr>\r\n                    {%- if doc.transporter_name -%}\r\n                    <td><strong> Transporter/Driver:</strong></td>\r\n                    <td>{{doc.transporter_name}}</td>\r\n                    {%- endif-%}\r\n                    {%- if doc.lr_date -%}\r\n                    <td><strong> Vehicle Entry Date:</strong></td>\r\n                    <td>{{doc.lr_date}}</td>\r\n                    {%- endif-%}\r\n                </tr>\r\n            </table>\r\n        </div>\r\n\r\n        <div>\r\n            <table id=\"table2\" style=\"margin-top: 15px;font-size: 12px;\">\r\n                <tbody>\r\n                    <tr>\r\n                        <th>Sl</th>\r\n                        <th> Code</th>\r\n                        <th> Name</th>\r\n                        <th> Quantity</th>\r\n                        <th>Rate</th>\r\n                        <th>Amount</th>\r\n                    </tr>\r\n                    {%- set symbol_currency = frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") -%}\r\n                    {%- for row in doc.items -%}\r\n                    <tr>\r\n                        <td>{{ row.idx }}</td>\r\n                        <td style=\"width: auto;\">{{ row.item_code }}</td>\r\n                        <td style=\"width: auto;\">{{row.item_name }}\r\n                             {% if row.item_name|trim != row.description|striptags %}\r\n                                {{ row.description }}\r\n                            {% endif %}\r\n                        </td>\r\n                        <td style=\" text-align: center;\">{{ row.qty }}</td>\r\n                        <td style=\" text-align: right;\">{{ frappe.utils.fmt_money(row.rate,currency=symbol_currency) }}\r\n                        </td>\r\n                        <td style=\"text-align: right;\">{{ frappe.utils.fmt_money(row.amount,currency=symbol_currency) }}\r\n                        </td>\r\n                    </tr>\r\n                    {%- endfor -%}\r\n                    <!-- <tr>\r\n                        <td style=\"text-align:right;\" colspan=\"5\"><b>Total</b></td>\r\n                        <td style=\"text-align: right\"><b>{{\r\n                                frappe.format_value(doc.total,{'fieldtype':'Currency'}) }}</b></td>\r\n                    </tr> -->\r\n\r\n                    {%- if doc.total_advance -%}\r\n                    <tr>\r\n                        <td colspan=\"5\" style=\"text-align:right;\"><b>Advance</b></td>\r\n                        <td style=\"text-align: right\">{{\r\n                            frappe.utils.fmt_money(doc.total_advance,currency=symbol_currency) }}</td>\r\n                    </tr>\r\n                    {%- endif -%}\r\n                    {%- if doc.discount_amount -%}\r\n                    <tr>\r\n                        <td colspan=\"5\" style=\"text-align:right;\">Discount</td>\r\n                        <td style=\"text-align:right \">{{\r\n                            frappe.utils.fmt_money(doc.discount_amount,currency=symbol_currency) }}</td>\r\n                    </tr>\r\n                    {%- endif -%}\r\n                    {%- for tax in doc.taxes -%}\r\n                    {%- if tax.tax_amount -%}\r\n                    <tr>\r\n                        <td colspan=\"5\" style=\"text-align:right;\">{{tax.title}}</td>\r\n                        <td style=\"text-align: right\">{{\r\n                            frappe.utils.fmt_money(tax.tax_amount,currency=symbol_currency) }}</td>\r\n                    </tr>\r\n                    {%- endif -%}\r\n                    {%- endfor -%}\r\n                    <tr>\r\n                        <td colspan=\"5\" style=\"text-align:right;\"><b>Total Amount</b></td>\r\n                        <td style=\"text-align:right\"><b style=\"text-align: right\">{{\r\n                                frappe.utils.fmt_money(doc.grand_total,currency=symbol_currency)}}</b></td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n\r\n            <div style=\"float: left; width: 100%; margin-top: 20px;\"><strong>In Words: </strong> \r\n            {{ frappe.utils.money_in_words(doc.grand_total, doc.currency) }}\r\n            </div>\r\n\r\n\r\n            <div style=\"padding-top:65px; clear: both;\">Authorized Signatory</div>\r\n            <!--<div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Delivered By) </div>-->\r\n            <!--<div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Carrier) </div>-->\r\n            <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">\r\n                Received By <br />\r\n                <!--<b>{{frappe.get_fullname(doc.owner)}}</b>-->\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"footer-html\" class=\"visible-pdf\">\r\n        {% if not no_letterhead and footer %}\r\n            <div class=\"letter-head-footer\">\r\n            {{ footer }}\r\n            </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n    <div style=\"padding-top: 100px; clear: both;\">\r\n        <div class=\"printedby\">This print is system generated and should be considered VALID only with appropriate seal\r\n            and/or signature. <br> Printed by {{ frappe.user }} on {{ frappe.utils.nowdate() }} at {{\r\n            frappe.utils.nowtime() }}\r\n        </div>\r\n    </div>\r\n",
